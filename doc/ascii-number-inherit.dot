digraph "ascii-number-inherit"
{
  graph [ label = "\G", fontcolor = red ];
  graph [ rankdir = "LR",
        center = true,
        size = "10,7.5" ];
  edge [ dir = "back",
       style = "dotted" ];

  "null-instance" [ label = "null",
                  shape = "plaintext" ];

  node [ shape = "box",
       style = "rounded",
       width = "2.0" ];

  "object"        -> "klass";
  "object"        -> "null";
  "null-instance" -> "object";

  "object"        -> "ascii-number";
  "klass"         -> "ascii-number-klass";
  "ascii-number"  -> "base-2-ascii-number";
  "ascii-number"  -> "base-64-ascii-number";
}
