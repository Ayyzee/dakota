digraph "collection-api-parts"
{
  graph [ label = "\G", fontcolor = red ];
  graph [ rankdir = "LR",
          center  = true,
          size    = "7.5,10" ];

  node [ shape = rect ];

  n3 [ label = "add\lremove\lreplace\l" ];
  n6 [ label = "first\llast\lbefore\lafter\lat\l" ];

  n6 -> "?";
  n3 -> "all" -> n6;
  n3 -> n6;
}
