digraph "collection-composition"
{
	graph [ label = "\G", fontcolor = red ];
	graph [ rankdir = LR, page = "8.5,11", size = "7.5,10", ratio = fill ];
	node [ style = rounded, shape = record, width = 1.75 ];
        edge [ dir = back ];

	"table" [ style = dashed ];
	"counted-set" [ style = dashed ];
	"set" [ style = dashed ];

	"sorted-set" -> "sorted-counted-set" [ color = green ];
	"hashed-set" -> "hashed-table" [ color = blue ];
	"sorted-set" -> "sorted-table" [ color = green ];
	"hashed-set" -> "hashed-counted-set" [ color = blue ];

	"set" -> "hashed-set" [ style = dashed ];
	"sorted-set" -> "set" [ style = dashed, dir = forward ];
        subgraph { graph [ rank = same ]; "hashed-set"; "set"; "sorted-set"};

	"counted-set" -> "sorted-counted-set" [ style = dashed ];
	"hashed-counted-set" -> "counted-set" [ style = dashed, dir = forward  ];
        subgraph { graph [ rank = same ]; "hashed-counted-set"; "counted-set"; "sorted-counted-set"};

	"table" -> "sorted-table" [ style = dashed ];
	"hashed-table" -> "table" [ style = dashed, dir = forward ];
        subgraph { graph [ rank = same ]; "hashed-table"; "table"; "sorted-table"};

        "collection" -> "hashed-set";
        "collection" -> "sorted-set";
}
