digraph "dylan-collection"
{
  graph [ label = "\G", fontcolor = red ];
  graph
    [
     rankdir = "LR",
     page = "8.5,11",
     size = "7.5,9.75",
     center = true,
     //ratio = "fill"
  ];
  node [ shape = rect ];
  edge [ dir = back ];

  "collection" -> "sequence";
  "collection" -> "mutable-collection";
  "mutable-collection" -> "mutable-sequence";
  "sequence" -> "mutable-sequence";

  "collection" -> "explicit-key-collection";
  "explicit-key-collection" -> "mutable-explicit-key-collection";
  "mutable-collection" -> "mutable-explicit-key-collection";

  "mutable-explicit-key-collection" -> "table";

  "collection" -> "stretchy-collection";
  "stretchy-collection" -> "table";

  "stretchy-collection" -> "stretchy-vector";
  "stretchy-collection" -> "deque";

  "mutable-sequence" -> "deque";
  "mutable-sequence" -> "array";
  "mutable-sequence" -> "string";

  "array" -> "vector";
}
