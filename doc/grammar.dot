  165 -> 170 [ label = "superklass" ];
  165 -> 180 [ label = "qual-ident", fontcolor = blue ];
  165 -> 270 [ label = "trait" ];
  180 -> 160 [ label = ";", fontname = "Courier-Bold", color = green ];
