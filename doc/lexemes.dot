digraph "lexems"
{
  graph [ label = "\G", fontcolor = red ];
  graph [ rankdir = "LR",
        center = true,
        size = "10,7.5" ];

  node [ shape = circle ];

  start [ label = "", shape = doublecircle ];
  start -> s01 [ label = "/" ];
  s01 -> s02 [ label = "/" ];
  s01 -> s03 [ label = "*" ];
  start -> s04 [ label = "[0-9]" ];
  s04 -> s04 [ label = "[0-9]" ];
  start -> s07 [ label = "$" ];

  s07 -> s05 [ label = "[_a-z]" ];
  s05 -> s05 [ label = "[_a-z0-9]" ];

  start -> s10 [ label = "[_a-z]" ];
  s10 -> s10 [ label = "[_a-z0-9]" ];

  start -> s08 [ label = ":" ];
  s08 -> s09 [ label = ":" ];

  //start -> s06 [ label = "[](){},;*&" ];

  start -> s12 [ label = "#" ];

  start -> s13 [ label = "\"" ];
  start -> s14 [ label = "\'" ];

  start -> s15 [ label = "\\" ];
  s15 -> s16 [ label = "\\" ];
}
