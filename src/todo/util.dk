// -*- mode: C++; c-basic-offset: 2; tab-width: 2; indent-tabs-mode: nil -*-

object-t set-from-collection(object-t klass, object-t collection)
{
  object-t result = dk:make(klass);

  for (object-t element : collection)
  {
    dk:add(result, element);
  }
  return result;
}

object-t sequence-from-collection(object-t klass, object-t collection)
{
  object-t result = dk:make(klass);

  for (object-t element : collection)
  {
    dk:add-last(result, element);
  }
  return result;
}
