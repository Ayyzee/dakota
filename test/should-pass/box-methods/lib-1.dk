// -*- mode: Dakota; c-basic-offset: 2; tab-width: 2; indent-tabs-mode: nil -*-

include <stdint.h>;

module test-1;

klass bk
{
  slots
  {
    int32-t s1;
  }

  method slots-t* max(slots-t* s, slots-t* other-s)
  {
    slots-t* result;
    if (s->s1 >= other-s->s1)
      result = s;
    else
      result = other-s;
    return result;
  }
}
