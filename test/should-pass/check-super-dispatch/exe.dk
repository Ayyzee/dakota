// -*- mode: Dakota; c-basic-offset: 2; tab-width: 2; indent-tabs-mode: nil -*-

 klass k1
{
  method object-t init(object-t self)
  {
    self = dk:init(super);
    log-klass-func();
    return self;
  }
}

 klass k2
{
  superklass k1;

  method object-t init(object-t self)
  {
    self = dk:init(super);
    log-klass-func();
    return self;
  }
}

 klass k3
{
  superklass k2;

  method object-t init(object-t self)
  {
    self = dk:init(super);
    log-klass-func();
    return self;
  }
}

int-t main()
{
  object-t o = make(k3:klass);

  char8-t chain[1024] = "";
  dk-get-klass-chain(o->klass, chain, sizeof(chain) - 1);
  printf("%s\n", chain);

  return 0;
}
