klass features; // hackhack

int-t main()
{
  object-t vector1 = make(vector:klass);
  dk:add-last(vector1, $"0");

  object-t vector2 = make(vector:klass);
  dk:add-last(vector2, $"1");
  dk:add-last(vector2, $"2");
  dk:add-last(vector2, $"3");

  dk:add-all-first(vector1, vector2);

  object-t perl-object-output-stream = make(perl-object-output-stream:klass);
  dk:write(perl-object-output-stream, vector1);
  dk:close(perl-object-output-stream);

  return 0;
}
