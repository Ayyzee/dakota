// -*- mode: Dakota; c-basic-offset: 2; tab-width: 2; indent-tabs-mode: nil -*-

#include <assert.h>

klass k1
{
  method object-t init(object-t self, uint32-t count : 0)
  {
    self = dk::init(super);
    assert(0x0c0c0c0c == count);
    //
    return self;
  }
  method object-t foo(object-t self, object-t bar : NULL)
  {
    assert((object-t)0xf0f0f0f0 == bar);
    //
    return self;
  }
}

int-t main()
{
  object-t o = make(k1::klass, count : 0x0c0c0c0c);
  dk::foo(o, bar : 0xf0f0f0f0);
  return 0;
}
