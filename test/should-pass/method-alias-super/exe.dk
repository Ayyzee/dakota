// -*- mode: Dakota; c-basic-offset: 2; tab-width: 2; indent-tabs-mode: nil -*-

include <assert.h>;

klass k1
{
  method alias(hash) uintmax-t alias-hash(object-t);
}

int-t main()
{
  selector-t hash-selector = selector(hash(object-t));
  method-t hash = dk::method-for-selector(k1::klass, hash-selector);
  selector-t alias-hash-selector = selector(alias-hash(object-t));
  method-t alias-hash = dk::method-for-selector(k1::klass, alias-hash-selector);
  assert(hash == alias-hash);
  dk-dump-methods(k1::klass);
  return 0;
}
