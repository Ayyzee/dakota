// -*- mode: Dakota; c-basic-offset: 2; tab-width: 2; indent-tabs-mode: nil -*-

klass features; // hackhack
klass vector;
klass json-object-output-stream;

int-t main() {
  object-t vector1 = make(vector::klass);
  $add-last(vector1, #"0");

  object-t vector2 = make(vector::klass);
  $add-last(vector2, #"1");
  $add-last(vector2, #"2");
  $add-last(vector2, #"3");

  $add-all-first(vector1, vector2);

  object-t json-object-output-stream = make(json-object-output-stream::klass);
  $write(json-object-output-stream, vector1);
  $close(json-object-output-stream);

  return 0;
}
