// -*- mode: Dakota; c-basic-offset: 2; tab-width: 2; indent-tabs-mode: nil -*-

klass deque;
trait stack;

int-t main() {
  int-t value = 0;

  try
  {
    object-t stack = make(deque::klass);
    $push(stack, null);
    object-t object = $pop(stack);
    USE(object);
    // hackhack: should test for null
  }
  catch(...) {
    fprintf(stderr, "ERROR: uncaught exception\n");
    value = 1;
  }
  return value;
}
