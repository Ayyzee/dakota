// -*- mode: Dakota; c-basic-offset: 2; tab-width: 2; indent-tabs-mode: nil -*-

klass json-object-output-stream;
klass json-parser;

int-t main()
{
  object-t parser = make(json-parser::klass);
  object-t property-list = dk::read-property-list(parser);
  fprintf(stderr, "---\n");
  object-t output-stream = make(json-object-output-stream::klass);
  dk::write(output-stream, property-list);
  dk::close(output-stream);
  fprintf(stderr, "---\n");
  return 0;
}
