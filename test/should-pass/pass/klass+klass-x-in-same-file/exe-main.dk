// -*- mode: Dakota; c-basic-offset: 2; tab-width: 2; indent-tabs-mode: nil -*-

klass output-stream;

klass foobar {
  method foo(object-t self) -> object-t {
    $print-format(std-output, "%s:%s(%s)\n", __klass__, __method__->name, __method__->parameter-types);
    return self;
  }
}
klass foobar {
  method bar(object-t self) -> object-t {
    $print-format(std-output, "%s:%s(%s)\n", __klass__, __method__->name, __method__->parameter-types);
    return self;
  }
}
int-t main() {
  object-t o = make(foobar::klass);

  $bar(o);
  $foo(o);

  return 0;
}
