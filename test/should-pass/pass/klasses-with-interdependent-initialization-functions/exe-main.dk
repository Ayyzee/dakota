// -*- mode: Dakota; c-basic-offset: 2; tab-width: 2; indent-tabs-mode: nil -*-

# include <assert.h>

klass k1 {
  initialize(object-t kls) -> object-t {
    dk-klass-for-name(#k2);
    return kls;
  }
}
klass k2 {
  initialize(object-t kls) -> object-t {
    dk-klass-for-name(#k1);
    return kls;
  }
}
int-t main() {
  object-t k1 = make(k1::klass);
  object-t k2 = make(k2::klass);
  USE(k1);
  USE(k2);
  return 0;
}
