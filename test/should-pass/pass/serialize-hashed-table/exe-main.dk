// -*- mode: Dakota; c-basic-offset: 2; tab-width: 2; indent-tabs-mode: nil -*-

klass features; // hackhack
klass hashed-table;
klass json-object-output-stream;

int-t main()
{
  object-t st = make(hashed-table::klass);

  dk::add(st, #"key0", #"element0");

  object-t json-object-output-stream = make(json-object-output-stream::klass);
  dk::write(json-object-output-stream, st);

  dk::close(json-object-output-stream);
  return 0;
}
