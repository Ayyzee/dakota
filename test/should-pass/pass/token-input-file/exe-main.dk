// -*- mode: Dakota; c-basic-offset: 2; tab-width: 2; indent-tabs-mode: nil -*-

# include <ctype.h>
# include <assert.h>

klass input-file;

static boole-t space?(int-t c) {
  return isspace(c);
}
int-t main(int-t argc, const char8-t* const* argv) {
  const char8-t* const* filenames = &argv[1];
  assert(nullptr != *filenames);
  const char8-t* filename;

  while (nullptr != (filename = *filenames++)) {
    object-t f = make(input-file::klass, #filename : filename);
    object-t t = $split(f, space?);
    $dump(t);
  }
  return 0;
}
