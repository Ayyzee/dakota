// -*- mode: Dakota; c-basic-offset: 2; tab-width: 2; indent-tabs-mode: nil -*-

klass exception;
klass str;
klass system-exception;

int-t main() {
  try {
    printf("try\n");
  }
  catch (int e0) {
    USE(e0);
  }
  catch (str-t e1) {
    USE(e1);
  }
  catch (system-exception::klass e2) {
    USE(e2);
  }
  catch (exception::klass e3) {
    USE(e3);
  }
  catch (...) {
    printf("catch: %p\n", _exception_);
    throw;
  }
  finally {
    printf("finally\n");
  }
  return 0;
}
