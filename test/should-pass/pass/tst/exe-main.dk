module dakota-util;

klass hashed-table;

int-t main() {
  finally {
    printf("finally works\n");
  }
  object-t o = make(hashed-table::klass);
  $dump(o);
  $println(o, std-output);
  printf("klass=%s\n", $utf8-from(klass-of(o)));
  printf("superklass=%s\n", $utf8-from($superklass(klass-of(o))));
  $dump($superklass(klass-of(o)));
  # if defined DEBUG
  char8-t buf[1024] = "";
  printf("klass-chain=%s\n", dkt-get-klass-chain(klass-of(o), buf, sizeof(buf)));
  # endif

  if (null) {
    # if defined DEBUG
    printf("klass-chain=%s\n", dkt-get-klass-chain(klass-of(null), buf, sizeof(buf)));
    # endif
    object-t null-klass = null->klass;
    object-t second-null = make(null-klass);
    $dump(null);
    printf("---\n");
    $dump(second-null);
    printf("---\n");
    $dump(klass-of(null));
    printf("---\n");
    $dump(klass-of(second-null));
  }
  return 0;
}
