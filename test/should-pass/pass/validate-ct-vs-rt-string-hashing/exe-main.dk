// -*- mode: Dakota; c-basic-offset: 2; tab-width: 2; indent-tabs-mode: nil -*-

# include <stdio.h>
# include <assert.h>

int-t main() {
  uintmax-t h = #'foobar';
  symbol-t s = #foobar;
  object-t o = symbol::box(s);
  printf("$hash(#foobar) = 0x%x, __hash::_foobar = 0x%x\n",
         (uint-t)$hash(o), (uint-t)h);
  assert((uint-t)$hash(o) == (uint-t)h);
  return 0;
}
